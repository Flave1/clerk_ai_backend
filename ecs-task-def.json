{
    "family": "clerk_backend_task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::588412562130:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::588412562130:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "clerk_backend",
            "image": "588412562130.dkr.ecr.us-east-1.amazonaws.com/clerk_backend:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "ENVIRONMENT",
                    "value": "production"
                },
                {
                    "name": "AWS_REGION",
                    "value": "us-east-1"
                },
                {
                    "name": "DEBUG",
                    "value": "false"
                },
                {
                    "name": "API_HOST",
                    "value": "0.0.0.0"
                },
                {
                    "name": "API_PORT",
                    "value": "8000"
                },
                {
                    "name": "API_PREFIX",
                    "value": "/api/v1"
                },
                {
                    "name": "LIVEKIT_URL",
                    "value": "wss://your-livekit-url.livekit.cloud"
                },
                {
                    "name": "LIVEKIT_API_KEY",
                    "value": "your_livekit_api_key_here"
                },
                {
                    "name": "LIVEKIT_API_SECRET",
                    "value": "your_livekit_api_secret_here"
                },
                {
                    "name": "LIVEKIT_VOICE_WEBHOOK_SECRET",
                    "value": ""
                },
                {
                    "name": "PHONE_NUMBER",
                    "value": "+1234567890"
                },
                {
                    "name": "WEBHOOK_BASE_URL",
                    "value": "https://your-production-domain.com"
                },
                {
                    "name": "OPENAI_API_KEY",
                    "value": "sk-proj-your-openai-key-here"
                },
                {
                    "name": "ANTHROPIC_API_KEY",
                    "value": "sk-ant-your-anthropic-key-here"
                },
                {
                    "name": "ELEVENLABS_API_KEY",
                    "value": "sk_your-elevenlabs-key-here"
                },
                {
                    "name": "WHISPER_MODEL",
                    "value": "base"
                },
                {
                    "name": "AWS_POLLY_VOICE_ID",
                    "value": "Joanna"
                },
                {
                    "name": "GOOGLE_OAUTH_CLIENT_CONFIG",
                    "value": "services/workers/tools/properties/clerk_demo_calender_secrets.json"
                },
                {
                    "name": "GOOGLE_CLIENT_ID",
                    "value": "your_google_client_id_here.apps.googleusercontent.com"
                },
                {
                    "name": "GOOGLE_CLIENT_SECRET",
                    "value": "your_google_client_secret_here"
                },
                {
                    "name": "ZOOM_ACCOUNT_ID",
                    "value": "your_zoom_account_id_here"
                },
                {
                    "name": "ZOOM_CLIENT_ID",
                    "value": "your_zoom_client_id_here"
                },
                {
                    "name": "ZOOM_CLIENT_SECRET",
                    "value": "your_zoom_client_secret_here"
                },
                {
                    "name": "ZOOM_REDIRECT_URI",
                    "value": "https://your-production-domain.com/auth/zoom/callback"
                },
                {
                    "name": "MS_CLIENT_ID",
                    "value": "your_microsoft_client_id_here"
                },
                {
                    "name": "MS_CLIENT_SECRET",
                    "value": "your_microsoft_client_secret_here"
                },
                {
                    "name": "MS_TENANT_ID",
                    "value": "your_microsoft_tenant_id_here"
                },
                {
                    "name": "AI_EMAIL",
                    "value": "your_ai_email@yourdomain.com"
                },
                {
                    "name": "API_BASE_URL",
                    "value": "https://your-production-domain.com"
                },
                {
                    "name": "SLACK_BOT_TOKEN",
                    "value": "xoxp-your-slack-token-here"
                },
                {
                    "name": "SMTP_HOST",
                    "value": "smtp.gmail.com"
                },
                {
                    "name": "SMTP_PORT",
                    "value": "587"
                },
                {
                    "name": "SMTP_USERNAME",
                    "value": "your_email@gmail.com"
                },
                {
                    "name": "SMTP_PASSWORD",
                    "value": "your_smtp_password_here"
                },
                {
                    "name": "MEETING_AUTO_JOIN_ENABLED",
                    "value": "true"
                },
                {
                    "name": "MEETING_JOIN_BUFFER_MINUTES",
                    "value": "5"
                },
                {
                    "name": "MEETING_MAX_JOIN_ATTEMPTS",
                    "value": "3"
                },
                {
                    "name": "MEETING_TRANSCRIPTION_ENABLED",
                    "value": "true"
                },
                {
                    "name": "MEETING_SUMMARIZATION_ENABLED",
                    "value": "true"
                },
                {
                    "name": "MEETING_EMAIL_NOTIFICATIONS_ENABLED",
                    "value": "true"
                },
                {
                    "name": "MEETING_SLACK_NOTIFICATIONS_ENABLED",
                    "value": "true"
                },
                {
                    "name": "SECRET_KEY",
                    "value": "your-secret-key-for-jwt-tokens"
                },
                {
                    "name": "ALGORITHM",
                    "value": "HS256"
                },
                {
                    "name": "ACCESS_TOKEN_EXPIRE_MINUTES",
                    "value": "30"
                },
                {
                    "name": "TWILIO_ACCOUNT_SID",
                    "value": "your_twilio_account_sid_here"
                },
                {
                    "name": "TWILIO_AUTH_TOKEN",
                    "value": "your_twilio_auth_token_here"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/clerk_backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}